{
    "manifest_version": 2,
    "name": "Website Time Tracker",
    "version": "1.0",
    "description": "Track time spent on different websites",
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html"
    }
    
  }